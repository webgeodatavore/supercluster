{
  "name": "supercluster",
  "version": "2.1.0",
  "description": "A crazy fast geospatial point clustering library.",
  "browser": "dist/supercluster.js",
  "main": "dist/supercluster.js",
  "scripts": {
    "pretest": "eslint index.js test/test.js",
    "test": "tap test",
    "cov": "tap test --cov",
    "uglifyjs": "cat dist/supercluster.js |uglifyjs > dist/supercluster.min.js",
    "compile:js": "browserify index.js -s supercluster > dist/supercluster.js",
    "compile:all": "mkdir -p dist && npm run compile:js && npm run uglifyjs",
    "mypublish": "npm run compile:all && npm publish"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/supercluster.git"
  },
  "keywords": [
    "clustering",
    "geospatial",
    "markers"
  ],
  "author": "Vladimir Agafonkin",
  "license": "ISC",
  "dependencies": {
    "kdbush": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "eslint": "^2.7.0",
    "eslint-config-mourner": "^2.0.1",
    "tap": "^5.7.1",
    "uglifyjs": "^2.4.10"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "camelcase": 0
    }
  }
}
